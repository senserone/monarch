<%@ page import="com.fh.common.Office" %>



<div class="fieldcontain ${hasErrors(bean: officeInstance, field: 'title', 'error')} required">
	<label for="title">
		<g:message code="office.title.label" default="Title" />
		<span class="required-indicator">*</span>
	</label>
	<g:textField name="title" required="" value="${officeInstance?.title}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: officeInstance, field: 'prefixCode', 'error')} required">
	<label for="prefixCode">
		<g:message code="office.prefixCode.label" default="Prefix Code" />
		<span class="required-indicator">*</span>
	</label>
	<g:textField name="prefixCode" required="" value="${officeInstance?.prefixCode}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: officeInstance, field: 'logoTitle', 'error')} ">
	<label for="logoTitle">
		<g:message code="office.logoTitle.label" default="Logo Title" />
		
	</label>
	<g:textField name="logoTitle" value="${officeInstance?.logoTitle}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: officeInstance, field: 'address', 'error')} ">
	<label for="address">
		<g:message code="office.address.label" default="Address" />
		
	</label>
	<g:textArea name="address" cols="40" rows="5" maxlength="1500" value="${officeInstance?.address}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: officeInstance, field: 'faxNumber', 'error')} ">
	<label for="faxNumber">
		<g:message code="office.faxNumber.label" default="Fax Number" />
		
	</label>
	<g:textField name="faxNumber" value="${officeInstance?.faxNumber}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: officeInstance, field: 'email', 'error')} ">
	<label for="email">
		<g:message code="office.email.label" default="Email" />
		
	</label>
	<g:textField name="email" value="${officeInstance?.email}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: officeInstance, field: 'vatNumber', 'error')} ">
	<label for="vatNumber">
		<g:message code="office.vatNumber.label" default="Vat Number" />
		
	</label>
	<g:textField name="vatNumber" value="${officeInstance?.vatNumber}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: officeInstance, field: 'phoneNumber', 'error')} ">
	<label for="phoneNumber">
		<g:message code="office.phoneNumber.label" default="Phone Number" />
		
	</label>
	<g:textField name="phoneNumber" value="${officeInstance?.phoneNumber}"/>
</div>

